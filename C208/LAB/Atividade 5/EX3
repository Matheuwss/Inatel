#EXERCÍCIO 3
.text
li $t0, 324		#0001 0100 0100
li $t1, 100		#0000 0110 0100
li $t2, 2018		#0111 1110 0010
li $t3, 1970		#0111 1011 0010
li $t4, 33		#0000 0010 0001
li $t5, 66		#0000 0100 0010

or $s0,$t0,$t1		#$s0 -> $t0 | $t1	0001 0110 0100 -> 356
or $s1,$t2,$t3		#$s1 -> $t2 | $t3	0111 1111 0010 -> 2034
or $s2,$t4,$t5		#$s2 -> $t4 | $t5	0000 0110 0011 -> 99